{
  "name": "miniapp-render",
  "version": "0.0.1",
  "description": "web's adapter for miniprogram",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "test": "npm run build && jest --bail",
    "test-debug": "npm run build && node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
    "coverage": "npm run build && jest --coverage --bail",
    "codecov": "npm run build && jest --coverage && codecov"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/**/__tests__/**/*.test.js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wechat-miniprogram/kbone.git     "
  },
  "author": "wechat-miniprogram",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "cross-env": "^5.2.1",
    "jest": "^24.8.0",
    "jsdom": "^15.1.1",
    "miniprogram-simulate": "^1.1.5",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.1.0",
    "rollup-plugin-replace": "^2.2.0"
  }
}
